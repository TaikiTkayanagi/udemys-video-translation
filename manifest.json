{
    "manifest_version": 3,
    "name": "udemy-translate-English-to-Japanese",
    "version": "1.0.0",
    "description": "説明",
    "permissions": [
        "storage",
        "contextMenus"
    ],
    "background": {
        "service_worker": "script/background.js",
        "type": "module"
    },
    "content_scripts": [
        {
            "js": [
                "script/content.js"
            ],
            "matches": [
                "https://newdaysysjp.udemy.com/course/*/learn/*"
            ]
        }
    ]
}
