{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "typescript",
			"tsconfig": "tsconfig.json",
			"problemMatcher": [
				"$tsc"
			],
			"group": "build",
			"label": "tsc: build - tsconfig.json"
		},
		{
			"type": "shell",
			"command": ["cp"],
			"args": ["${workspaceFolder}/manifest.json", "${workspaceFolder}/script/"],
			"dependsOn": ["tsc: build - tsconfig.json"],
			"label": "copy manifest.json"
		}
	]
}
